        // Note that VSCode is licensed under Microsoft's Own License.
        // Install it means you agreed MS's License
        //"Code",
        // VNC based on noVNC
        "VNC"
    ],
    "Features": [
        // Desktop GUI, disable this will cause WebOS running in TTY Mode.
        "Desktop",
        // Background, disable this will disable the background replacement.
        "Background"
    ]
}